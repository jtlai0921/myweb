<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

   <bean id = "m" class = "Model.member">
     	<property name="name" value="teacher"></property>
     	<property name="address" value="台北"></property>
     	<property name="interest">
     		<list>
     			<value>音樂</value>
     			<value>運動</value>
     			<value>電影</value>
     		</list>   	
     	
     	</property>
   </bean>
   <bean id="p" class="Model.porder">
   		<property name="name" value="teacher"/>
   		<property name="product">
   			<list>
   				<value>Mouse</value>
   				<value>Lcd</value>
   				<value>Ram</value>
   			</list>
   		
   		</property>
   
   </bean>
   
   <bean id="MF" class="Model.ModelFactory" autowire="byType"/>

</beans>